# نسخ هذا الملف إلى .env.local وإضافة القيم الفعلية

# مفتاح Google AI للحصول على التفسير
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# تفعيل تحليلات Vercel
NEXT_PUBLIC_VERCEL_ANALYTICS=true

# تعطيل جمع البيانات الاستخدامية لـ Next.js
NEXT_TELEMETRY_DISABLED=1

# بيئة التطبيق
NODE_ENV=production